// Initial Template for C++

#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
// User function Template for C++

class Solution {
  public:
    void rotate(vector<int> &arr) {
        // code here
        int i=0, j=arr.size()-1;
        while(i!=j){
            swap(arr[i],arr[j]);
            i++;
        }
        
    }
};
